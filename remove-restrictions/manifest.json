{

   "content_scripts": [
     {
      "matches": [
         "http://www.bellinghamherald.com/*",
         "http://seattletimes.com/*", 
         "http://www.nytimes.com/*", 
         "http://blogs.seattletimes.com/*"],

       "css": ["global/css/remove.css"],
       "js": ["global/js/minjq.js", "global/js/remove.js"]

     },
     {
       "matches": ["http://www.independent.co.uk/*"],
       "css": ["global/css/remove.css"],
       "js": ["global/js/minjq.js", "js/independent_co_uk/remove.js"]
     }
   ],

   "description": "Remove those annoying popup modals and 'MUST REGISTER NOW' requests.",
   "key": "",
   "manifest_version": 2,
   "name": "Remove restrictions",
   "permissions": [ 
      "http://www.nytimes.com/*", 
      "http://blog.seattletimes.com/*", 
      "http://seattletimes.com/*", 
      "http://www.bellinghamherald.com/*",
      "http://www.independent.co.uk/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.2"

}
